<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <array name="com_google_android_gms_fonts_certs">
        <item>@array/com_google_android_gms_fonts_certs_dev</item>
        <item>@array/com_google_android_gms_fonts_certs_prod</item>
    </array>
    <string-array name="com_google_android_gms_fonts_certs_dev">
        <item>
            
        </item>
    </string-array>
    <string-array name="com_google_android_gms_fonts_certs_prod">
        <item>
            
        </item>
    </string-array>
    <array name="preloaded_fonts" translatable="false">
        <item>@font/alfa_slab_one</item>
        <item>@font/audiowide</item>
    </array>
    <color name="background">#232323</color>
    <color name="black">#FF000000</color>
    <color name="ecoColor">#26A52C </color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="redCircleColor">#da291c</color>
    <color name="smoke_grey">#848884 </color>
    <color name="teal_200">#EB262D</color>
    <color name="teal_700">#FF018786</color>
    <color name="textColor">#323232</color>
    <color name="textColorDark">#5e5e5e</color>
    <color name="textLightColor">#999999</color>
    <color name="tourColor">#FF5F1F </color>
    <color name="transparent">#0000ffff </color>
    <color name="white">#FFFFFFFF</color>
    <color name="yellow">#FFD700 </color>
    <string name="app_name">RE_Cluster_App</string>
    <style name="Theme.RecieverApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        
        <item name="fontFamily">@font/audiowide_regular</item>
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
    </style>
    <declare-styleable name="MyCustomView">
        <attr format="color" name="needlecolor"/>
        <attr format="color" name="linecolor"/>
        <attr format="reference|color" name="backimage"/>
        <attr format="boolean" name="removeborder"/>

    </declare-styleable>
    <declare-styleable name="TickerView">
        <attr format="reference|integer" name="ticker_animationDuration"/>
        <attr format="reference|boolean" name="ticker_animateMeasurementChange"/>
        <attr format="enum" name="ticker_defaultCharacterList">
            <enum name="number" value="1"/>
            <enum name="alphabet" value="2"/>
        </attr>
        <attr format="enum" name="ticker_defaultPreferredScrollingDirection">
            <enum name="any" value="0"/>
            <enum name="up" value="1"/>
            <enum name="down" value="2"/>
        </attr>

        
        <attr name="android:gravity" ns1:ignore="ResourceName"/>
        <attr name="android:shadowColor" ns1:ignore="ResourceName"/>
        <attr name="android:shadowDx" ns1:ignore="ResourceName"/>
        <attr name="android:shadowDy" ns1:ignore="ResourceName"/>
        <attr name="android:shadowRadius" ns1:ignore="ResourceName"/>
        <attr name="android:text" ns1:ignore="ResourceName"/>
        <attr name="android:textAppearance" ns1:ignore="ResourceName"/>
        <attr name="android:textColor" ns1:ignore="ResourceName"/>
        <attr name="android:textSize" ns1:ignore="ResourceName"/>
        <attr name="android:textStyle" ns1:ignore="ResourceName"/>
        <attr name="android:fontFamily" ns1:ignore="ResourceName"/>
        <attr name="android:font" ns1:ignore="ResourceName"/>
        <attr name="android:maxLength" ns1:ignore="ResouceName"/>
    </declare-styleable>
    <declare-styleable name="ec_arc_progress">
        <attr format="color" name="ec_arc_container_color"/>
        <attr format="color" name="ec_arc_progress_color"/>
        <attr format="dimension" name="ec_arc_text_size"/>
        <attr format="color" name="ec_arc_text_color"/>
        <attr format="string" name="ec_arc_text"/>
        <attr format="integer" name="ec_arc_max"/>
        <attr format="integer" name="ec_arc_current"/>
        <attr format="dimension" name="ec_arc_unit_text_size"/>
        <attr format="color" name="ec_arc_unit_text_color"/>
        <attr format="string" name="ec_arc_unit_text"/>
        <attr format="string" name="ec_hud_orientation"/>
        <attr format="integer" name="ec_vb_progress"/>
        <attr format="integer" name="ec_vb_max"/>
    </declare-styleable>
</resources>